    "start": "yarn relay && cross-env GENERATE_SOURCEMAP=false react-scripts --max_old_space_size=8192 start",
    "generate": "cross-env GENERATE_SOURCEMAP=false PUBLIC_URL=%BASE_PATH% react-scripts --max-old-space-size=7168 build",
    "build": "yarn relay && yarn generate && node src/setupBuild.js",
    "lint": "eslint --max-warnings 0 ./src",
    "control": "yarn audit --groups dependencies --summary",
    "relay": "relay-compiler",
    "test": "react-scripts test --coverage --watchAll=false"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.graphql.js",
      "!src/setup*.js",
      "!src/index.js",
      "!<rootDir>/node_modules/"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
