{
  "app": {
    "port": 4000,
    "logs_directory": "./logs"
  },
  "db": {
    "uri": "bolt://",
    "user": "setup your user",
    "password": "setup your password"
  },
  "jwt": {
    "secret": "define your secret"
  },
  "providers": {
    "facebook": {
      "client_id": "FACEBOOK_APP_ID",
      "client_secret": "FACEBOOK_APP_SECRET",
      "callback_uri": "http://www.example.com/auth/facebook/callback"
    },
    "google" : {
      "client_id": "GOOGLE_CONSUMER_KEY",
      "client_secret": "GOOGLE_CONSUMER_SECRET",
      "callback_uri": "http://www.example.com/auth/google/callback"
    },
    "github" : {
      "client_id": "GITHUB_CONSUMER_KEY",
      "client_secret": "GITHUB_CONSUMER_SECRET",
      "callback_uri": "http://www.example.com/github/google/callback"
    }
  }
}