{
  "name": "opencti-graphql",
  "version": "1.0.0",
  "main": "src/server.js",
  "author": "Luatix",
  "license": "MIT",
  "scripts": {
    "start": "babel-node -r node_modules/dotenv/config ./src/server.js",
    "build": "babel --out-dir ./dist ./src && pkg package.json --output ./build/opencti",
    "serv": "node dist/server.js",
    "schema": "babel-node ./src/schema/schema-init.js",
    "schema:clear-rules": "babel-node ./src/schema/schema-clear-rules.js",
    "migrate": "babel-node ./src/database/migration.js",
    "migrate:add": "migrate create",
    "lint": "eslint .",
    "test": "jest --coverage",
    "clean": "babel-node ./src/database/clean.js"
  },
  "bin": "./dist/server.js",
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "config/**/*",
      "public/**/*"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@godaddy/terminus": "^4.1.0",
    "apollo-errors": "^1.9.0",
    "apollo-server-express": "^2.3.1",
    "await": "^0.2.6",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bytes": "^3.1.0",
    "compression": "^1.7.3",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "grakn": "^1.3.1",
    "graphql": "^14.1.1",
    "graphql-constraint-directive": "^1.3.0",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-redis-subscriptions": "^2.0.0",
    "graphql-relay": "^0.5.5",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.3",
    "helmet": "^3.15.0",
    "ioredis": "^4.3.0",
    "iterall": "^1.2.2",
    "jsonwebtoken": "^8.4.0",
    "merge-graphql-schemas": "^1.5.8",
    "migrate": "^1.6.2",
    "moment": "^2.23.0",
    "nconf": "^0.10.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "ramda": "^0.26.1",
    "rxjs": "^6.3.3",
    "uuid": "^3.3.2",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-jest": "^23.6.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.5.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^23.6.0",
    "pkg": "^4.3.7",
    "prettier": "^1.14.3"
  }
}
